<!-- http://wiki.freeswitch.org/wiki/Mod_conference -->
<!-- None of these paths are real if you want any of these options you need to really set them up -->
<configuration name="imm_adapter.conf" description="Immersitech Adapter">
  <!-- Advertise certain presence on startup . -->
  <settings>
    <param name="license_file_path" value="/usr/local/lib/immersitech/Immersitech_Engineering_sound_manager_license_key.dat"/>
    <param name="room_layout_file_path" value="/usr/local/lib/immersitech/room_layout.json"/>
    <param name="websocket_config_file_path" value="/usr/local/lib/immersitech/imm_websocket_config.json"/>
    <param name="websocket_enabled" value="1"/>
    <param name="imm_output_audio_sample_rate" value="48000"/>
    <param name="imm_output_audio_frames_per_buffer" value="960"/>
    <param name="imm_spatial_quality" value="5"/>
  </settings>
</configuration>
